<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="mainPane" maxHeight="-Infinity" maxWidth="-Infinity"
    minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="800.0"
    xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="application.Controller">
    <center>
        <VBox alignment="CENTER" prefHeight="500.0" prefWidth="800.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="registrazioneButton" alignment="TOP_LEFT" mnemonicParsing="false" text="Registrazione">
                    <VBox.margin>
                        <Insets bottom="15.0" />
                    </VBox.margin>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </Button>
                <Button fx:id="loginButton" alignment="CENTER" mnemonicParsing="false" text="Login">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </Button>
            </children>
        </VBox>
    </center>
</BorderPane>
